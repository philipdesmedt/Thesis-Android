load "AndroidComponent"

AndroidComponent smsComponent {
	
	AComp SMS {
		package = "be.pds.sms";
		gencodetemplate = "sms.egl";
		ctype = "sms";

		properties = [phone, message];
	}

	ComponentData phone {
		name = "phone";
		value = "5664";
	}

	ComponentData message {
		name = "message";
		value = "This is a default text message.";
	}
}
